npm remove -g nixar
lsc -c nixar.ls
cd commands
lsc -c * -o ../compiled-commands
cd ..
jq .subversion=.subversion+1 package.json > output.json
jq .version=.uperversion+\".\"+\(.subversion\|tostring\) output.json > package.json
rm output.json
#git add -u
#git add .
#git commit -m "Development"
#git push origin master
npm publish
npm install -g nixar
echo "done"
